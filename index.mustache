    <footer class="footer anchor" id="contact">
        <div class="footer__flex-wr">
            <p class="legal__subtitle legal__subtitle--footer">{{contactTitle}}</p>
            <div class="footer-content">
                <div class="footer__phone">
                    <a href="tel:+74992319820" class="footer__phone-desc"><i class="fas fa-phone"></i>+7 499 231 98
