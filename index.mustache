                    <a class="header-top-menu__item_link" href="#players">
                        <li class="header-top-menu__item">{{menuPlay}}</li>
                    </a>
                    <a class="header-top-menu__item_link" href="#commands">
                        <li class="header-top-menu__item">{{menuCommand}}</li>
                    </a>
                    <a class="header-top-menu__item_link" href="#operator">
                        <li class="header-top-menu__item">{{menuOperator}}</li>
                    </a>
                    <a class="header-top-menu__item_link" href="#talent">
                        <li class="header-top-menu__item">{{menuTalent}}</li>
                    </a>
                    <a class="header-top-menu__item_link" href="#studio">
                        <li class="header-top-menu__item">{{menuStudio}}</li>
                    </a>
                    <a class="header-top-menu__item_link" href="#internet">
                        <li class="header-top-menu__item">{{menuInternet}}</li>
                    </a>
                    <a class="header-top-menu__item_link" href="#developers">
                        <li class="header-top-menu__item">{{menuDevelopers}}</li>
                    </a>
                    <a class="header-top-menu__item_link" href="#backoffice">
                        <li class="header-top-menu__item">{{menuBackOffice}}</li>
                    </a>
                    <a class="header-top-menu__item_link" href="#contact">
                        <li class="header-top-menu__item">{{menuContacts}}</li>
                    </a>
                </ul>
                <div class="header-top-menu-contact">
                    <div class="lang-selector">
                        <a class="lang-selector--link {{langClassRU}}" href="index.html">RU</a>
                        <a class="lang-selector--link {{langClassENG}}" href="english.html">ENG</a>
                    </div>
                    <a href="tel:+74992319820" class="header-top-menu-contact__link">+7 499 231 98 20</a>
                </div>
                <div class="header-top-menu__mobile header-top-menu__mobile--js" id="buttonMenu">
                    <div class="header-top-menu__mobile-item"></div>
                    <div class="header-top-menu__mobile-item"></div>
                    <div class="header-top-menu__mobile-item"></div>
                </div>
            </div>
